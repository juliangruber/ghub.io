    "predirect": "~1.0.0"
  },
  "devDependencies": {
    "webtap": "~0.1.0"
  },
  "keywords": [
    "npm",
    "git",
    "github",
    "website",
    "redirect"
  ],
  "author": {
    "name": "Julian Gruber",
    "email": "mail@juliangruber.com",
    "url": "http://juliangruber.com"
  },
  "license": "MIT",
  "engines": {
    "node": "0.8.x"
  },
  "subdomain": "ghub",
