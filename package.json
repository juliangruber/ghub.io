{
  "name": "ghub",
  "private": true,
  "description": "ghub.io/<package-name> takes you to the package's github page",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/juliangruber/ghub.io.git"
  },
  "homepage": "http://ghub.io/",
  "main": "index.js",
  "scripts": {
    "start": {
      "ghub.io": "bin/ghub.io.js"
    }
  },
  "bin": {
    "ghub.io": "./bin/ghub.io.js"
  },
  "dependencies": {
    "ecstatic": "~0.4.0",
    "ga": "0.0.2",
    "github-url-to-object": "^1.5.2",
    "level-11": "^0.18.0",
    "level-couch-sync": "~1.0.2",
